{
    "name": "blue-ecommerce-theme/blue-ecommerce-theme",
    "description": "A modern blue e-commerce theme inheriting from Shopware 6 standard template",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "BlueEcommerceTheme\\": "src/"
        }
    },
    "require": {
        "shopware/core": "^6.0.0",
        "shopware/storefront": "^6.0.0"
    },
    "extra": {
        "shopware-plugin-class": "BlueEcommerceTheme\\BlueEcommerceTheme",
        "plugin-icon": "src/Resources/public/plugin.png",
        "label": {
            "de-DE": "Blue E-Commerce Theme",
            "en-GB": "Blue E-Commerce Theme"
        },
        "description": {
            "de-DE": "Ein modernes, blaues E-Commerce-Theme, das vom Shopware 6 Standard-Template erbt",
            "en-GB": "A modern blue e-commerce theme inheriting from Shopware 6 standard template"
        },
        "manufacturerLink": {
            "de-DE": "https://www.example.com",
            "en-GB": "https://www.example.com"
        },
        "supportLink": {
            "de-DE": "https://www.example.com/support",
            "en-GB": "https://www.example.com/support"
        }
    }
}
